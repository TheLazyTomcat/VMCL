Function Vector_EqualVectors({%H-}AutoTest: Boolean = False): Integer;
var
  v2s_s,v2s_d:  TVMCLVector2s;
  v3s_s,v3s_d:  TVMCLVector3s;
  v4s_s,v4s_d:  TVMCLVector4s;
  v2d_s,v2d_d:  TVMCLVector2d;
  v3d_s,v3d_d:  TVMCLVector3d;
  v4d_s,v4d_d:  TVMCLVector4d;
begin
WriteLn;
WriteLn(LineText('EqualVectors'));
RandomVec(v2s_s); RandomVec(v3s_s); RandomVec(v4s_s);
RandomVec(v2d_s); RandomVec(v3d_s); RandomVec(v4d_s);
RandomVec(v2s_d); RandomVec(v3s_d); RandomVec(v4s_d);
RandomVec(v2d_d); RandomVec(v3d_d); RandomVec(v4d_d);
WriteLn;
WriteLn('v2s:      ' + VecToStr(v2s_s) + ' = ' + VecToStr(v2s_d) + ' >> ' + BoolToStr(EqualVectors(v2s_s,v2s_d),True));
CopyVector(v2s_s,v2s_d);
WriteLn('          ' + VecToStr(v2s_s) + ' = ' + VecToStr(v2s_d) + ' >> ' + BoolToStr(EqualVectors(v2s_s,v2s_d),True));
WriteLn;
WriteLn('v3s:      ' + VecToStr(v3s_s) + ' = ' + VecToStr(v3s_d) + ' >> ' + BoolToStr(EqualVectors(v3s_s,v3s_d),True));
CopyVector(v3s_s,v3s_d);
WriteLn('          ' + VecToStr(v3s_s) + ' = ' + VecToStr(v3s_d) + ' >> ' + BoolToStr(EqualVectors(v3s_s,v3s_d),True));
WriteLn;
WriteLn('v4s:      ' + VecToStr(v4s_s) + ' = ' + VecToStr(v4s_d) + ' >> ' + BoolToStr(EqualVectors(v4s_s,v4s_d),True));
CopyVector(v4s_s,v4s_d);
WriteLn('          ' + VecToStr(v4s_s) + ' = ' + VecToStr(v4s_d) + ' >> ' + BoolToStr(EqualVectors(v4s_s,v4s_d),True));
WriteLn;
RandomVec(v4s_s); RandomVec(v4s_d);
WriteLn('v4s(XYZ): ' + VecToStr(v4s_s) + ' = ' + VecToStr(v4s_d) + ' >> ' + BoolToStr(EqualVectorsXYZ(v4s_s,v4s_d),True));
CopyVectorXYZ(v4s_s,v4s_d);
WriteLn('          ' + VecToStr(v4s_s) + ' = ' + VecToStr(v4s_d) + ' >> ' + BoolToStr(EqualVectorsXYZ(v4s_s,v4s_d),True));
WriteLn;
WriteLn('v2d:      ' + VecToStr(v2d_s) + ' = ' + VecToStr(v2d_d) + ' >> ' + BoolToStr(EqualVectors(v2d_s,v2d_d),True));
CopyVector(v2d_s,v2d_d);
WriteLn('          ' + VecToStr(v2d_s) + ' = ' + VecToStr(v2d_d) + ' >> ' + BoolToStr(EqualVectors(v2d_s,v2d_d),True));
WriteLn;
WriteLn('v3d:      ' + VecToStr(v3d_s) + ' = ' + VecToStr(v3d_d) + ' >> ' + BoolToStr(EqualVectors(v3d_s,v3d_d),True));
CopyVector(v3d_s,v3d_d);
WriteLn('          ' + VecToStr(v3d_s) + ' = ' + VecToStr(v3d_d) + ' >> ' + BoolToStr(EqualVectors(v3d_s,v3d_d),True));
WriteLn;
WriteLn('v4d:      ' + VecToStr(v4d_s) + ' = ' + VecToStr(v4d_d) + ' >> ' + BoolToStr(EqualVectors(v4d_s,v4d_d),True));
CopyVector(v4d_s,v4d_d);
WriteLn('          ' + VecToStr(v4d_s) + ' = ' + VecToStr(v4d_d) + ' >> ' + BoolToStr(EqualVectors(v4d_s,v4d_d),True));
WriteLn;
RandomVec(v4d_s); RandomVec(v4d_d);
WriteLn('v4d(XYZ): ' + VecToStr(v4d_s) + ' = ' + VecToStr(v4d_d) + ' >> ' + BoolToStr(EqualVectorsXYZ(v4d_s,v4d_d),True));
CopyVectorXYZ(v4d_s,v4d_d);
WriteLn('          ' + VecToStr(v4d_s) + ' = ' + VecToStr(v4d_d) + ' >> ' + BoolToStr(EqualVectorsXYZ(v4d_s,v4d_d),True));
Result := 0;
end;
