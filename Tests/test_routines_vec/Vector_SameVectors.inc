Function Vector_SameVectors({%H-}AutoTest: Boolean = False): Integer;
var
  v2s_s,v2s_d:  TVector2s;
  v3s_s,v3s_d:  TVector3s;
  v4s_s,v4s_d:  TVector4s;
  v2d_s,v2d_d:  TVector2d;
  v3d_s,v3d_d:  TVector3d;
  v4d_s,v4d_d:  TVector4d;
  Epss,Epsd:    Single;
begin
WriteLn;
WriteLn(LineText('SameVectors'));
Epss := Random(100); Epsd := Random(100);
WriteLn;
WriteLn(Format('Epsilon: %fs %fd',[Epss,Epsd]));
WriteLn;
RandomVec(v2s_s); RandomVec(v3s_s); RandomVec(v4s_s);
RandomVec(v2d_s); RandomVec(v3d_s); RandomVec(v4d_s);
RandomVec(v2s_d); RandomVec(v3s_d); RandomVec(v4s_d);
RandomVec(v2d_d); RandomVec(v3d_d); RandomVec(v4d_d);
WriteLn('v2s:      ' + VecToStr(v2s_s) + ' = ' + VecToStr(v2s_d) + ' >> ' + BoolToStr(SameVectors(v2s_s,v2s_d,Epss),True));
CopyVector(v2s_s,v2s_d);
WriteLn('          ' + VecToStr(v2s_s) + ' = ' + VecToStr(v2s_d) + ' >> ' + BoolToStr(SameVectors(v2s_s,v2s_d,Epss),True));
WriteLn;
WriteLn('v3s:      ' + VecToStr(v3s_s) + ' = ' + VecToStr(v3s_d) + ' >> ' + BoolToStr(SameVectors(v3s_s,v3s_d,Epss),True));
CopyVector(v3s_s,v3s_d);
WriteLn('          ' + VecToStr(v3s_s) + ' = ' + VecToStr(v3s_d) + ' >> ' + BoolToStr(SameVectors(v3s_s,v3s_d,Epss),True));
WriteLn;
WriteLn('v4s:      ' + VecToStr(v4s_s) + ' = ' + VecToStr(v4s_d) + ' >> ' + BoolToStr(SameVectors(v4s_s,v4s_d,Epss),True));
CopyVector(v4s_s,v4s_d);
WriteLn('          ' + VecToStr(v4s_s) + ' = ' + VecToStr(v4s_d) + ' >> ' + BoolToStr(SameVectors(v4s_s,v4s_d,Epss),True));
WriteLn;
RandomVec(v4s_s); RandomVec(v4s_d);
WriteLn('v4s(XYZ): ' + VecToStr(v4s_s) + ' = ' + VecToStr(v4s_d) + ' >> ' + BoolToStr(SameVectorsXYZ(v4s_s,v4s_d,Epss),True));
CopyVectorXYZ(v4s_s,v4s_d);
WriteLn('          ' + VecToStr(v4s_s) + ' = ' + VecToStr(v4s_d) + ' >> ' + BoolToStr(SameVectorsXYZ(v4s_s,v4s_d,Epss),True));
WriteLn;
WriteLn('v2d:      ' + VecToStr(v2d_s) + ' = ' + VecToStr(v2d_d) + ' >> ' + BoolToStr(SameVectors(v2d_s,v2d_d,Epsd),True));
CopyVector(v2d_s,v2d_d);
WriteLn('          ' + VecToStr(v2d_s) + ' = ' + VecToStr(v2d_d) + ' >> ' + BoolToStr(SameVectors(v2d_s,v2d_d,Epsd),True));
WriteLn;
WriteLn('v3d:      ' + VecToStr(v3d_s) + ' = ' + VecToStr(v3d_d) + ' >> ' + BoolToStr(SameVectors(v3d_s,v3d_d,Epsd),True));
CopyVector(v3d_s,v3d_d);
WriteLn('          ' + VecToStr(v3d_s) + ' = ' + VecToStr(v3d_d) + ' >> ' + BoolToStr(SameVectors(v3d_s,v3d_d,Epsd),True));
WriteLn;
WriteLn('v4d:      ' + VecToStr(v4d_s) + ' = ' + VecToStr(v4d_d) + ' >> ' + BoolToStr(SameVectors(v4d_s,v4d_d,Epsd),True));
CopyVector(v4d_s,v4d_d);
WriteLn('          ' + VecToStr(v4d_s) + ' = ' + VecToStr(v4d_d) + ' >> ' + BoolToStr(SameVectors(v4d_s,v4d_d,Epsd),True));
WriteLn;
RandomVec(v4d_s); RandomVec(v4d_d);
WriteLn('v4d(XYZ): ' + VecToStr(v4d_s) + ' = ' + VecToStr(v4d_d) + ' >> ' + BoolToStr(SameVectorsXYZ(v4d_s,v4d_d,Epsd),True));
CopyVectorXYZ(v4d_s,v4d_d);
WriteLn('          ' + VecToStr(v4d_s) + ' = ' + VecToStr(v4d_d) + ' >> ' + BoolToStr(SameVectorsXYZ(v4d_s,v4d_d,Epsd),True));
Result := 0;
end;
